$(function(){
  var punchCards = '<%= j render partial: "punch_cards", locals: {clock_events: @clock_events} if @clock_events.present? %>'
  var punchButton = '<%= j render partial: "punch_button", locals: {clock_event: @clock_event, punch_out: @punch_out, name: @name} %>'
  var animate = '<%= @animate %>'

  Callbacks.showResults(punchCards, punchButton, animate)
})
